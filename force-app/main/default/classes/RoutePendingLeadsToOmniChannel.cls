public with sharing class RoutePendingLeadsToOmniChannel implements Queueable {

    /*
     * Constructor
     */
    public RoutePendingLeadsToOmniChannel() {

        system.debug('RoutePendingLeadsToOmniChannel ctor');
    }

    /******************************************************************
    * Method: execute, public
    * Description: Execute the Queueable interface
    * Return: void
    *******************************************************************/
    public void execute(QueueableContext context) {

        try {

            System.debug('Inside RoutePendingLeadsToOmniChannel Execute');

            String est_pending_queue_dev_name = Utils.getHippoSettings('Pending Leads EST Queue');
            String pst_pending_queue_dev_name = Utils.getHippoSettings('Pending Leads PST Queue');
            Id estPendingLeadsQueue = Utils.getQueueId(est_pending_queue_dev_name);
            Id pstPendingLeadsQueue = Utils.getQueueId(pst_pending_queue_dev_name);

            String batch_size = Utils.getHippoSettings('Timezone Routing Batch Size');

            System.debug('Timezone Routing Batch Size: ' + batch_size);

            // List all pending leads, order by Day_Of_Week_Created__c DESC (Weekend's first) and CreatedDate DESC
            //String query = 'SELECT Id, CreatedDate, Pending_Queue_Id__c, Day_Of_Week_Created__c From Lead Where Pending_Queue_Id__c != \'\' AND Day_Of_Week_Created__c != \'\' ORDER BY Day_Of_Week_Created__c DESC NULLS LAST, CreatedDate DESC Limit ' + batch_size;

            // List all pending leads, order by CreatedDate DESC
            String query = 'SELECT Id, OwnerId, State, CreatedDate, Pending_Queue_Id__c From Lead Where Pending_Queue_Id__c != \'\' ORDER BY CreatedDate DESC Limit ' + batch_size;

            System.debug('RoutePendingLeadsToOmniChannel Query: ' + query);

            List<UserServicePresence> available_Agents = Utils.getAvailableOmniChannelAgents();

            Boolean anyLeadRouted = false;

            if ( available_Agents.size() > 0 || Test.isRunningTest() ) { // Only distribute leads if there are available online agents

                List<Lead> leads_in_Context = Database.query(query);

                Map<String, Utils.StateInfo> map_State2Coast = Utils.getStates2StateInfo();

                for (Lead my_lead : leads_in_Context) {

                    Boolean isWithin = false;

                    String state = my_lead.State?.toUpperCase();

                    if (String.isNotBlank(state) && map_State2Coast.get(state).coast.toLowerCase().equals('west')) {

                        isWithin = Utils.isWithinPST(system.now()) || QACockpit.instance.qacp_Force_Inside_Business_Hours_for_PST.enabled;
                    }
                    else if (String.isNotBlank(state) && map_State2Coast.get(state).coast.toLowerCase().equals('east')) {

                        isWithin = Utils.isWithinEST(system.now()) || QACockpit.instance.qacp_Force_Inside_Business_Hours_for_EST.enabled;
                    }
                    // During a test execution it is not possible to enforce a workable time so just assume 'inside business hours' during test execution
                    if ( (isWithin && (my_lead.OwnerId?.equals(estPendingLeadsQueue) || my_lead.OwnerId?.equals(pstPendingLeadsQueue)) ) || Test.isRunningTest()  ) {

                        my_lead.OwnerId = my_lead.Pending_Queue_Id__c;
                        my_lead.Pending_Queue_Id__c = '';
                        anyLeadRouted = true;
                    }
                    else if ( String.isNotBlank(my_lead.Pending_Queue_Id__c) && !my_lead.OwnerId?.equals(estPendingLeadsQueue) && !my_lead.OwnerId?.equals(pstPendingLeadsQueue) ) {

                        my_lead.Pending_Queue_Id__c = '';
                        anyLeadRouted = true;
                    }

                    /*
                    String state = my_lead.State;

                    if (String.isNotBlank(state) && map_State2Coast.containsKey(state)) {

                        if ( (map_State2Coast.get(state).toLowerCase().equals('west')) ||
                             (map_State2Coast.get(state).toLowerCase().equals('east')) ) {

                            Boolean isWithin = Utils.isWithinPST(system.now()) || Utils.isWithinEST(system.now());
                            if ( isWithin ) {

                                my_lead.OwnerId = my_lead.Pending_Queue_Id__c;
                                my_lead.Pending_Queue_Id__c = '';
                                //my_lead.Day_Of_Week_Created__c = '';
                                anyLeadRouted = true;
                            }
                        }
                    }
                     */
                }

                if (leads_in_Context.size() > 0 && anyLeadRouted) {

                    LeadTriggerHandler.SKIP_RECURSION = true;
                    List<Database.SaveResult> sr = Database.update(leads_in_Context);
                    LeadTriggerHandler.SKIP_RECURSION = false;
                    Utils.debugSaveResults(sr);

                    if (leads_in_Context.size() == Integer.valueOf(batch_size)) {

                        if (!Test.isRunningTest()) {

                            Id jobId = System.enqueueJob(new RoutePendingLeadsToOmniChannel());
                            System.debug('Chaining additional RoutePendingLeadsToOmniChannel, jobId is: ' + jobId);

                        } else {

                            RoutePendingLeadsToOmniChannel routePendingLeadsToOmniChannel = new RoutePendingLeadsToOmniChannel();
                            routePendingLeadsToOmniChannel.execute(null);
                        }
                    }
                }
            }
        }
        catch(Exception ex) {

            System.debug('Exception from RoutePendingLeadsToOmniChannel execution: ' + ex.getMessage());
        }
    }

